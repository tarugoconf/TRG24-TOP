{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.3.3/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.7.1/",
    "top/": "https://cdn.jsdelivr.net/gh/tarugoconf/top@11a86cab090362d0a7e389778251d42396bc5485/"
  },
  "lock": false,
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
